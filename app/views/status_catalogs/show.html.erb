<div class="page-header">
  <%= link_to status_catalogs_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_status_catalog_path(@status_catalog), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show status_catalog</h1>
</div>

<dl class="dl-horizontal">
  <dt>Status catalog:</dt>
  <dd><%= @status_catalog.status_catalog_id %></dd>

  <dt>Status name:</dt>
  <dd><%= @status_catalog.status_name %></dd>

</dl>
