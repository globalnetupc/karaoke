    <%= f.label :local_video, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :local_video, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
