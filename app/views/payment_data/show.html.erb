<div class="page-header">
  <%= link_to payment_data_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_payment_datum_path(@payment_datum), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show payment_datum</h1>
</div>

<dl class="dl-horizontal">
  <dt>Payment data:</dt>
  <dd><%= @payment_datum.payment_data_id %></dd>

  <dt>Payment type:</dt>
  <dd><%= @payment_datum.payment_type_id %></dd>

  <dt>Data name:</dt>
  <dd><%= @payment_datum.data_name %></dd>

  <dt>Data type:</dt>
  <dd><%= @payment_datum.data_type %></dd>

</dl>
