<div class="page-header">
  <%= link_to generes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_genere_path(@genere), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show genere</h1>
</div>

<dl class="dl-horizontal">
  <dt>Genere:</dt>
  <dd><%= @genere.genere_id %></dd>

  <dt>Name:</dt>
  <dd><%= @genere.name %></dd>

  <dt>Description:</dt>
  <dd><%= @genere.description %></dd>

</dl>
