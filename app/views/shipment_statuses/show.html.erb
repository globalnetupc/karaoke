<div class="page-header">
  <%= link_to shipment_statuses_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_shipment_status_path(@shipment_status), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show shipment_status</h1>
</div>

<dl class="dl-horizontal">
  <dt>Shipment:</dt>
  <dd><%= @shipment_status.shipment_id %></dd>

  <dt>Status catalog:</dt>
  <dd><%= @shipment_status.status_catalog_id %></dd>

  <dt>Status time:</dt>
  <dd><%= @shipment_status.status_time %></dd>

  <dt>Notes:</dt>
  <dd><%= @shipment_status.notes %></dd>

</dl>
